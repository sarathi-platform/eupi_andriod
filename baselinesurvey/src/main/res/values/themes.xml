<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Android_starter_project" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/grey_lighter</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Android_starter_project_blow_lollipop" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/grey_lighter</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowIsTranslucent">false</item>
        <!-- Customize your theme here. -->
    </style>
</resources>